{
  "name": "jupyter-js-output-area",
  "version": "0.3.0",
  "description": "Library for handling and displaying Jupyter output messages.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf docs && rimraf lib && rimraf test/build",
    "clean:example": "rimraf example/build",
    "build:example": "tsc --project example/src && webpack --config example/webpack.conf.js",
    "build:src": "tsc --project src && node scripts/copycss.js",
    "build:test": "tsc --project test/src && webpack --config test/webpack.conf.js",
    "build": "npm run build:src && npm run build:test",
    "docs": "typedoc --options scripts/tdoptions.json",
    "postinstall": "npm dedupe",
    "prepublish": "npm run build",
    "test:chrome": "karma start --browsers=Chrome test/karma.conf.js",
    "test:coverage": "webpack --config test/webpack-cov.conf.js && karma start test/karma-cov.conf.js",
    "test:firefox": "karma start --browsers=Firefox test/karma.conf.js",
    "test:ie": "karma start --browsers=IE test/karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true test/karma.conf.js",
    "test": "npm run test:firefox",
    "watch": "npm run watch:build -s",
    "watch:build": "nodemon -q -w src/ --ext ts,js --exec 'npm run build'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jupyter/jupyter-js-output-area.git"
  },
  "keywords": [
    "jupyter",
    "js",
    "frontend",
    "front",
    "end",
    "output",
    "area",
    "outputarea",
    "display",
    "mimetype",
    "mime",
    "notebook",
    "component",
    "message",
    "msg"
  ],
  "author": "Project Jupyter",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyter/jupyter-js-output-area/issues"
  },
  "homepage": "https://github.com/jupyter/jupyter-js-output-area#readme",
  "dependencies": {
    "phosphor-disposable": "^1.0.5",
    "phosphor-observablelist": "^1.0.0-beta",
    "phosphor-properties": "^2.0.0",
    "phosphor-signaling": "^1.2.0",
    "phosphor-widget": "^1.0.0-rc.0",
    "phosphor-panel": "^1.0.0-rc.1",
    "transformime": "^2.1.0",
    "transformime-jupyter-transformers": "^0.2.0"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "expect.js": "^0.3.1",
    "fs-extra": "^0.26.3",
    "istanbul-instrumenter-loader": "^0.1.3",
    "json-loader": "^0.5.4",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.0",
    "rimraf": "^2.4.2",
    "style-loader": "^0.13.0",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2",
    "webpack": "^1.12.9"
  }
}
